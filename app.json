{
  "expo": {
    "name": "lastSubway",
    "slug": "the-last-subway",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.letsdoyi.lastSubway"
    },
    "android": {
      "package": "com.letsdoyi.lastSubway",
      "googleServicesFile": "./google-services.json"
    }
  }
}
